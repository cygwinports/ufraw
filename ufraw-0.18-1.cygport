inherit gnome2

DESCRIPTION="Utility to read and manipulate digital camera RAW images"
HOMEPAGE="http://ufraw.sourceforge.net/"
SRC_URI="mirror://sourceforge/${PN}/${P}.tar.gz"

PKG_NAMES="${PN} gimp2-${PN}" # cinepaint-${PN}"
ufraw_CONTENTS='etc/ usr/bin/ usr/share/'
gimp2_ufraw_CONTENTS='usr/lib/gimp/'
cinepaint_ufraw_CONTENTS='usr/lib/cinepaint/'

CYGCONF_ARGS="--enable-mime"
MAKEOPTS+=" schemasdir=/etc/gconf/schemas"
